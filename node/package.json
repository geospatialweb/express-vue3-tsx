{
  "name": "node-vue3-tsx",
  "version": "1.0.0",
  "license": "MIT",
  "author": "John Campbell <johncampbell@geospatialweb.ca> <http://www.linkedin.com/in/geospatialweb>",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/geospatialweb/express-vue3-tsx"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "build": "tsc",
    "prod": "node build/app.js",
    "deploy": "npm test && npm run build",
    "dev": "npx nodemon src/app.ts",
    "debug": "npx nodemon --inspect src/app.ts",
    "prepare": "cd .. && husky install node/.husky",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint src --ext .ts --cache",
    "lint:fix": "eslint src --ext .ts,.tsx --cache --fix",
    "test": "jest src --passWithNoTests"
  },
  "dependencies": {
    "celebrate": "^15.0.0",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.12",
    "@types/pg": "^8.6.4",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
