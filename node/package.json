{
  "name": "express-vue3-tsx",
  "version": "1.0.0",
  "license": "MIT",
  "author": "John Campbell <johncampbell@geospatialweb.ca> <http://www.linkedin.com/in/geospatialweb>",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/geospatialweb/express-vue3-tsx"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "build": "tsc",
    "prod": "node build/app.js",
    "debug": "npx nodemon --inspect src/app.ts",
    "deploy": "npm run lint && npm test && npm run build",
    "dev": "npx nodemon src/app.ts",
    "prepare": "cd .. && husky install node/.husky",
    "lint": "eslint src --ext .ts --cache",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "celebrate": "^15.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/geojson": "^7946.0.10",
    "@types/jest": "^29.1.1",
    "@types/node": "^18.8.0",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
